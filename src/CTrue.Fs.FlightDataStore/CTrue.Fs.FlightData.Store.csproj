<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Proto\FsTrackPoint.proto" />
    <None Remove="Proto\v1\FsTrackPoint - Copy.proto" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.15.7" />
    <PackageReference Include="Google.Protobuf.Tools" Version="3.15.7" />
    <PackageReference Include="Grpc.Tools" Version="2.36.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CTrue.FlightData.Contracts\CTrue.Fs.FlightData.Contracts.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="Proto\FsTrackLogHeader.proto" GrpcServices="Client" />
    <Protobuf Include="Proto\v2\FsTrackPoint.proto" GrpcServices="Client" />
    <Protobuf Include="Proto\v1\FsTrackPoint.proto" GrpcServices="Client" />
  </ItemGroup>

</Project>
